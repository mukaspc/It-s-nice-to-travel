---
import Layout from "../../../layouts/Layout.astro";
import PlansLayout from "../../../components/plans/PlansLayout.astro";
import { GeneratedPlanView } from "../../../components/plans/GeneratedPlanView";

const { planId } = Astro.params;

if (!planId) {
  return Astro.redirect('/plans');
}
---

<Layout title="Generated Plan">
  <PlansLayout>
    <GeneratedPlanView client:load planId={planId} />
  </PlansLayout>
</Layout> 