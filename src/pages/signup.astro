---
import Layout from "../layouts/Layout.astro";
import { getSupabaseConfig } from "../db/supabase.client";

// Pobierz konfiguracjÄ™ Supabase
const supabaseConfig = getSupabaseConfig();
---

<Layout title="Sign Up - It's nice to travel">
  <div
    id="signup-page"
    data-page-type="signup"
    data-supabase-url={supabaseConfig?.supabaseUrl}
    data-supabase-key={supabaseConfig?.supabaseKey}
  >
  </div>

  <script src="../scripts/signup.ts"></script>
</Layout>

